{
  "expo": {
    "owner": "bassasaurus",
    "name": "d2lb",
    "slug": "direct2lobook",
    "orientation": "portrait",
    "icon": "./app/assets/app_icon.png",
    "splash": {
      "image": "./app/assets/D2LB_LOGO_Dark_top.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.direct2logbook"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.direct2logbook"
    },
    "web": {
      "favicon": "./app/assets/favicon.png",
      "plugins": [
        "sentry-expo"
      ]
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "direct2logbook",
            "project": "d2lb_mobile",
            "authToken": "d2f68a21e3724a6bafc8a04320573586aa7b56a9139d4106ace02180a5771ee7"
          }
        }
      ]
    },
    "plugins": [
      "sentry-expo"
    ],
    "extra": {
      "eas": {
        "projectId": "ce3c3db1-1f7f-442b-973d-9a74163e6073"
      }
    }
  }
}
